<script>
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// DATA STORAGE
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

let profiles = [];
let followList = [];
let celebrities = [];
let bios = [];
let captions = [];

let currentProfileIndex = 0;
let currentFollowIndex = 0;
let followCheckStates = {};
let counterValue = 0;
let countedSet = new Set();

let userIP = '';
let currentClientId = '';

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// TAB SWITCHING
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function switchTab(tab) {
  // Update tab buttons
  document.querySelectorAll('.nav-tab').forEach(btn => {
    btn.classList.toggle('active', btn.dataset.tab === tab);
  });
  
  // Update sections
  document.querySelectorAll('.section').forEach(sec => sec.classList.remove('active'));
  document.getElementById(tab + 'Section').classList.add('active');
  
  // Show/hide follow nav and counter
  const navControls = document.getElementById('navControls');
  const floatingCounter = document.getElementById('floatingCounter');
  
  if (tab === 'follow') {
    navControls.style.display = 'flex';
    floatingCounter.style.display = 'flex';
  } else {
    navControls.style.display = 'none';
    floatingCounter.style.display = 'none';
  }
}

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// CONFIG MANAGEMENT
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function getClients() {
  return JSON.parse(localStorage.getItem('clients') || '{}');
}

function saveClients(clients) {
  localStorage.setItem('clients', JSON.stringify(clients));
}

function getCurrentClientConfig() {
  if (!currentClientId) return null;
  return getClients()[currentClientId];
}

function loadClientSelector() {
  const select = document.getElementById('clientSelector');
  const clients = getClients();
  
  select.innerHTML = '<option value="">Select Client</option>';
  
  Object.entries(clients).forEach(([id, config]) => {
    const opt = document.createElement('option');
    opt.value = id;
    opt.textContent = config.name || id;
    select.appendChild(opt);
  });
  
  const lastClient = localStorage.getItem('lastClientId');
  if (lastClient && clients[lastClient]) {
    select.value = lastClient;
    currentClientId = lastClient;
    loadAllData();
  }
}

document.getElementById('clientSelector').addEventListener('change', function() {
  currentClientId = this.value;
  localStorage.setItem('lastClientId', currentClientId);
  if (currentClientId) loadAllData();
});

// Settings Modal
function openSettings() {
  const config = getCurrentClientConfig() || {};
  document.getElementById('cfgClientName').value = config.name || '';
  document.getElementById('cfgMainApi').value = config.mainApi || '';
  document.getElementById('cfg2faApi').value = config.twofaApi || '';
  document.getElementById('cfgRotateUrl').value = config.rotateUrl || '';
  document.getElementById('cfgFraudUser').value = config.fraudUser || '';
  document.getElementById('cfgFraudKey').value = config.fraudKey || '';
  renderSavedClients();
  document.getElementById('settingsModal').classList.add('show');
}

function closeSettings() {
  document.getElementById('settingsModal').classList.remove('show');
}

function saveSettings() {
  const name = document.getElementById('cfgClientName').value.trim();
  if (!name) { toast('âŒ Client name required'); return; }
  
  const clientId = currentClientId || 'client_' + Date.now();
  const clients = getClients();
  
  clients[clientId] = {
    name,
    mainApi: document.getElementById('cfgMainApi').value.trim(),
    twofaApi: document.getElementById('cfg2faApi').value.trim(),
    rotateUrl: document.getElementById('cfgRotateUrl').value.trim(),
    fraudUser: document.getElementById('cfgFraudUser').value.trim(),
    fraudKey: document.getElementById('cfgFraudKey').value.trim()
  };
  
  saveClients(clients);
  currentClientId = clientId;
  localStorage.setItem('lastClientId', clientId);
  loadClientSelector();
  document.getElementById('clientSelector').value = clientId;
  closeSettings();
  toast('âœ… Client saved!');
  loadAllData();
}

function renderSavedClients() {
  const container = document.getElementById('savedClientsList');
  const clients = getClients();
  
  if (!Object.keys(clients).length) {
    container.innerHTML = '<p style="color:var(--text-dim); font-size:12px;">No clients saved yet</p>';
    return;
  }
  
  container.innerHTML = Object.entries(clients).map(([id, config]) => `
    <div style="display:flex; justify-content:space-between; align-items:center; padding:10px; background:rgba(0,0,0,0.3); border-radius:8px; margin-bottom:6px;">
      <span style="font-weight:600; color:var(--accent);">${config.name}</span>
      <div style="display:flex; gap:6px;">
        <button onclick="editClient('${id}')" style="padding:4px 10px; background:var(--purple); border:none; border-radius:6px; color:#fff; font-size:11px; cursor:pointer;">Edit</button>
        <button onclick="deleteClient('${id}')" style="padding:4px 10px; background:#f44336; border:none; border-radius:6px; color:#fff; font-size:11px; cursor:pointer;">Ã—</button>
      </div>
    </div>
  `).join('');
}

function editClient(id) {
  currentClientId = id;
  const config = getClients()[id];
  document.getElementById('cfgClientName').value = config.name || '';
  document.getElementById('cfgMainApi').value = config.mainApi || '';
  document.getElementById('cfg2faApi').value = config.twofaApi || '';
  document.getElementById('cfgRotateUrl').value = config.rotateUrl || '';
  document.getElementById('cfgFraudUser').value = config.fraudUser || '';
  document.getElementById('cfgFraudKey').value = config.fraudKey || '';
}

function deleteClient(id) {
  if (!confirm('Delete this client?')) return;
  const clients = getClients();
  delete clients[id];
  saveClients(clients);
  if (currentClientId === id) {
    currentClientId = '';
    localStorage.removeItem('lastClientId');
  }
  loadClientSelector();
  renderSavedClients();
  toast('ğŸ—‘ï¸ Client deleted');
}

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// DATA LOADING
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

async function loadAllData() {
  const config = getCurrentClientConfig();
  if (!config || !config.mainApi) {
    toast('âš ï¸ Configure API first');
    return;
  }
  
  try {
    toast('ğŸ”„ Loading...');
    const res = await fetch(config.mainApi);
    const data = await res.json();
    
    if (data.profiles) profiles = data.profiles;
    else if (Array.isArray(data)) profiles = data;
    
    if (data.followList) {
      followList = data.followList;
      followCheckStates = {};
      followList.forEach((u, i) => { followCheckStates[i] = !!u.checked; });
    }
    
    if (data.celebrities) celebrities = data.celebrities;
    if (data.bios) bios = data.bios;
    if (data.captions) captions = data.captions;
    
    // Restore indices
    currentProfileIndex = Math.min(
      parseInt(localStorage.getItem(`${currentClientId}_profileIndex`) || '0'),
      profiles.length - 1
    );
    currentFollowIndex = Math.min(
      parseInt(localStorage.getItem(`${currentClientId}_followIndex`) || '0'),
      followList.length - 1
    );
    
    if (currentProfileIndex < 0) currentProfileIndex = 0;
    if (currentFollowIndex < 0) currentFollowIndex = 0;
    
    displayProfile();
    renderFollowList();
    updateStats();
    
    toast(`âœ… Loaded ${profiles.length} profiles, ${followList.length} follows`);
  } catch (err) {
    console.error('Load error:', err);
    toast('âŒ Load failed');
  }
}

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// PROFILE DISPLAY & NAVIGATION
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function displayProfile() {
  if (!profiles.length) return;
  const p = profiles[currentProfileIndex];
  
  document.getElementById('pContainer').textContent = p.container || p.name || 'â€”';
  document.getElementById('pUsername').textContent = p.username || p.oldusername || 'â€”';
  document.getElementById('p2FA').textContent = p.twofa_key || p.newusername || 'â€”';
  document.getElementById('pPassword').textContent = p.password || 'â€”';
  document.getElementById('profileIndex').textContent = currentProfileIndex + 1;
  document.getElementById('profileTotal').textContent = profiles.length;
  
  document.getElementById('secretInput').value = (p.twofa_key || p.newusername || '').trim();
  updateTOTP();
  
  localStorage.setItem(`${currentClientId}_profileIndex`, currentProfileIndex);
}

function prevProfile() { if (currentProfileIndex > 0) { currentProfileIndex--; displayProfile(); } }
function nextProfile() { if (currentProfileIndex < profiles.length - 1) { currentProfileIndex++; displayProfile(); } }
function firstProfile() { currentProfileIndex = 0; displayProfile(); toast('ğŸ¥‡ First profile'); }

function showProfileList() {
  const container = document.getElementById('profileListContainer');
  container.innerHTML = profiles.map((p, i) => `
    <div onclick="selectProfile(${i})" style="padding:12px; background:${i === currentProfileIndex ? 'var(--accent-dim)' : 'rgba(0,0,0,0.3)'}; border-radius:10px; margin-bottom:6px; cursor:pointer; border:1px solid ${i === currentProfileIndex ? 'var(--accent)' : 'transparent'};">
      <div style="font-weight:600; color:var(--cyan);">${p.username || p.oldusername || 'Unknown'}</div>
      <div style="font-size:11px; color:var(--text-dim);">${p.container || p.name || ''}</div>
    </div>
  `).join('');
  document.getElementById('profileModal').classList.add('show');
}

function selectProfile(i) { currentProfileIndex = i; displayProfile(); closeProfileModal(); }
function closeProfileModal() { document.getElementById('profileModal').classList.remove('show'); }

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// FOLLOW LIST
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function renderFollowList() {
  const container = document.getElementById('followList');
  
  if (!followList.length) {
    container.innerHTML = '<p style="color:var(--text-dim); text-align:center; padding:20px;">No follow list data</p>';
    return;
  }
  
  container.innerHTML = followList.map((u, i) => `
    <div class="follow-row ${i === currentFollowIndex ? 'active' : ''}" onclick="selectFollow(${i})">
      <input type="checkbox" ${followCheckStates[i] ? 'checked' : ''} onclick="event.stopPropagation(); toggleCheck(${i})" data-index="${i}">
      <button class="ig-btn" onclick="event.stopPropagation(); window.open('https://instagram.com/${u.username}','_blank')">IG</button>
      <span class="username">${u.username || '???'}</span>
      <span class="status">${u.status || ''}</span>
    </div>
  `).join('');
  
  updateFollowCurrent();
}

function selectFollow(i) {
  currentFollowIndex = i;
  localStorage.setItem(`${currentClientId}_followIndex`, currentFollowIndex);
  
  document.querySelectorAll('.follow-row').forEach((row, idx) => {
    row.classList.toggle('active', idx === i);
  });
  
  updateFollowCurrent();
  
  const activeRow = document.querySelector('.follow-row.active');
  if (activeRow) activeRow.scrollIntoView({ behavior: 'smooth', block: 'center' });
}

function updateFollowCurrent() {
  const user = followList[currentFollowIndex];
  document.getElementById('currentUsername').textContent = user?.username || 'â€”';
  document.getElementById('currentCheckbox').checked = !!followCheckStates[currentFollowIndex];
}

function prevFollow() {
  if (!followList.length) return;
  syncToSheet();
  
  let start = currentFollowIndex;
  do {
    currentFollowIndex = currentFollowIndex <= 0 ? followList.length - 1 : currentFollowIndex - 1;
  } while (followList[currentFollowIndex].status === 'Skip' && currentFollowIndex !== start);
  
  selectFollow(currentFollowIndex);
  openCurrentFollowIG();
}

function nextFollow() {
  if (!followList.length) return;
  syncToSheet();
  
  let start = currentFollowIndex;
  do {
    currentFollowIndex = (currentFollowIndex + 1) % followList.length;
  } while (followList[currentFollowIndex].status === 'Skip' && currentFollowIndex !== start);
  
  selectFollow(currentFollowIndex);
  openCurrentFollowIG();
}

function openCurrentFollowIG() {
  const user = followList[currentFollowIndex];
  if (user?.username) window.open(`https://instagram.com/${user.username}`, '_blank');
}

function toggleCheck(i) {
  followCheckStates[i] = !followCheckStates[i];
  
  if (followCheckStates[i] && !countedSet.has(i)) {
    counterValue++;
    countedSet.add(i);
  } else if (!followCheckStates[i] && countedSet.has(i)) {
    counterValue--;
    countedSet.delete(i);
  }
  
  updateCounter();
  updateFollowCurrent();
  syncToSheet();
}

function toggleCurrentCheckbox() {
  const checked = document.getElementById('currentCheckbox').checked;
  followCheckStates[currentFollowIndex] = checked;
  
  const listCb = document.querySelector(`.follow-row input[data-index="${currentFollowIndex}"]`);
  if (listCb) listCb.checked = checked;
  
  if (checked && !countedSet.has(currentFollowIndex)) {
    counterValue++;
    countedSet.add(currentFollowIndex);
  } else if (!checked && countedSet.has(currentFollowIndex)) {
    counterValue--;
    countedSet.delete(currentFollowIndex);
  }
  
  updateCounter();
  syncToSheet();
}

function searchFollow() {
  const query = document.getElementById('followSearch').value.trim().toLowerCase();
  if (!query) return;
  
  const found = followList.findIndex(u => u.username?.toLowerCase().includes(query));
  if (found !== -1) {
    selectFollow(found);
    toast(`â†’ ${followList[found].username}`);
  } else {
    toast('Not found');
  }
}

function updateStats() {
  document.getElementById('profileIndex').textContent = currentProfileIndex + 1;
  document.getElementById('profileTotal').textContent = profiles.length;
  
  const done = Object.values(followCheckStates).filter(v => v).length;
  document.getElementById('statTotal').textContent = followList.length;
  document.getElementById('statDone').textContent = done;
  document.getElementById('statRemaining').textContent = followList.length - done;
  
  document.getElementById('statBios').textContent = bios.length;
  document.getElementById('statCaptions').textContent = captions.length;
  document.getElementById('statCelebs').textContent = celebrities.length;
}

function updateCounter() {
  const el = document.getElementById('floatingCounter');
  const limit = parseInt(document.getElementById('followLimit').value || '0');
  
  el.textContent = counterValue;
  el.classList.remove('red');
  
  if (limit > 0 && counterValue >= limit) {
    el.classList.add('red');
  }
}

function resetCounter() {
  counterValue = 0;
  countedSet.clear();
  updateCounter();
  toast('Counter reset');
}

function copyCheckStates() {
  const txt = followList.map((_, i) => followCheckStates[i] ? 'TRUE' : 'FALSE').join('\n');
  navigator.clipboard.writeText(txt).then(() => toast('ğŸ“‹ Copied!'));
}

// Auto-sync with debounce
let syncTimeout = null;
function syncToSheet() {
  if (syncTimeout) clearTimeout(syncTimeout);
  
  syncTimeout = setTimeout(async () => {
    const config = getCurrentClientConfig();
    if (!config || !config.mainApi) return;
    
    const updates = followList.map((u, i) => ({
      username: u.username,
      checked: !!followCheckStates[i],
      row: i + 2
    }));
    
    try {
      const proxiedUrl = 'https://corsproxy.io/?' + encodeURIComponent(config.mainApi);
      await fetch(proxiedUrl, {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ action: 'updateFollow', data: updates })
      });
    } catch (e) {
      console.error('Sync failed:', e);
    }
  }, 500);
}

function manualSync() {
  syncToSheet();
  toast('âœ… Synced');
}

// Check IG account exists
async function checkIG() {
  const box = document.getElementById('igStatusBox');
  box.style.backgroundColor = 'gray';
  
  const username = followList[currentFollowIndex]?.username?.trim();
  if (!username) { box.style.backgroundColor = 'red'; return; }
  
  try {
    const res = await fetch(`https://www.instagram.com/api/v1/users/web_profile_info/?username=${username}`, {
      method: 'GET',
      headers: {
        'User-Agent': 'Mozilla/5.0 (iPhone; CPU iPhone OS 17_0 like Mac OS X)',
        'X-IG-App-ID': '936619743392459',
        'X-Requested-With': 'XMLHttpRequest',
        'Accept': '*/*'
      }
    });
    
    if (res.status === 200) {
      const data = await res.json();
      box.style.backgroundColor = data?.data?.user?.id ? 'lime' : 'red';
      box.style.boxShadow = data?.data?.user?.id ? '0 0 8px lime' : 'none';
    } else {
      box.style.backgroundColor = 'red';
    }
  } catch (e) {
    box.style.backgroundColor = 'red';
  }
}

function runNextContainer() {
  window.location.href = 'shortcuts://run-shortcut?name=IG%20Switch';
}

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// CONTENT
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function randomBio() {
  if (!bios.length) { toast('No bios loaded'); return; }
  const bio = bios[Math.floor(Math.random() * bios.length)];
  document.getElementById('randomBioText').textContent = bio;
  navigator.clipboard.writeText(bio);
  toast('ğŸŒ¸ Bio copied!');
}

function randomCaption() {
  if (!captions.length) { toast('No captions loaded'); return; }
  const cap = captions[Math.floor(Math.random() * captions.length)];
  document.getElementById('randomCaptionText').textContent = cap;
  navigator.clipboard.writeText(cap);
  toast('ğŸ“ Caption copied!');
}

function randomCelebrity() {
  if (!celebrities.length) { toast('No celebrities loaded'); return; }
  const celeb = celebrities[Math.floor(Math.random() * celebrities.length)];
  document.getElementById('randomCelebText').textContent = celeb;
  navigator.clipboard.writeText(celeb);
  window.open('https://www.instagram.com/explore/search/keyword/?q=' + encodeURIComponent(celeb), '_blank');
  toast('ğŸ² ' + celeb);
}

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// 2FA / TOTP
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function base32toBytes(b32) {
  b32 = (b32 || '').replace(/=+$/, '').replace(/[^A-Z2-7]/gi, '').toUpperCase();
  const alph = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ234567';
  let bits = 0, val = 0, bytes = [];
  for (let c of b32) {
    const idx = alph.indexOf(c);
    if (idx === -1) continue;
    val = (val << 5) | idx;
    bits += 5;
    if (bits >= 8) { bits -= 8; bytes.push((val >> bits) & 0xFF); }
  }
  return new Uint8Array(bytes);
}

async function generateTOTP(secret) {
  const keyBytes = base32toBytes(secret);
  if (!keyBytes.length) return null;
  
  const counter = Math.floor(Date.now() / 1000 / 30);
  const buf = new ArrayBuffer(8);
  new DataView(buf).setUint32(4, counter);
  
  const key = await crypto.subtle.importKey('raw', keyBytes, { name: 'HMAC', hash: 'SHA-1' }, false, ['sign']);
  const sig = new Uint8Array(await crypto.subtle.sign('HMAC', key, buf));
  
  const offset = sig[sig.length - 1] & 0x0f;
  const code = ((sig[offset] & 0x7f) << 24 | (sig[offset+1] & 0xff) << 16 | (sig[offset+2] & 0xff) << 8 | sig[offset+3] & 0xff) % 1000000;
  return code.toString().padStart(6, '0');
}

async function updateTOTP() {
  const secret = document.getElementById('secretInput').value.trim();
  document.getElementById('totpTimer').textContent = 30 - (Math.floor(Date.now() / 1000) % 30);
  
  if (!secret) { document.getElementById('totpCode').textContent = '------'; return; }
  document.getElementById('totpCode').textContent = await generateTOTP(secret) || 'invalid';
}

setInterval(updateTOTP, 1000);

async function pasteSecret() {
  try {
    document.getElementById('secretInput').value = (await navigator.clipboard.readText()).trim();
    updateTOTP();
    toast('ğŸ“‹ Pasted!');
  } catch (e) { toast('Paste blocked'); }
}

function copyTOTP() {
  const code = document.getElementById('totpCode').textContent.trim();
  if (code && code !== '------' && code !== 'invalid') {
    navigator.clipboard.writeText(code);
    toast('ğŸ” Code copied!');
  }
}

async function send2FAToSheet() {
  const config = getCurrentClientConfig();
  const apiUrl = config?.twofaApi || config?.mainApi;
  if (!apiUrl) { toast('âŒ Configure API first'); return; }
  
  const newSecret = document.getElementById('secretInput').value.trim();
  if (!newSecret) { toast('No secret entered'); return; }
  
  const currentUser = profiles[currentProfileIndex];
  if (!currentUser?.id) { toast('Profile has no ID'); return; }
  
  try {
    const res = await fetch(apiUrl, {
      method: 'POST',
      headers: { 'Content-Type': 'text/plain;charset=utf-8' },
      body: JSON.stringify({ action: 'update2FA', id: currentUser.id, twofa_key: newSecret })
    });
    const result = await res.json();
    
    if (result.success) {
      toast('âœ… 2FA saved!');
      currentUser.twofa_key = newSecret;
      document.getElementById('p2FA').textContent = newSecret;
    } else {
      toast('âŒ ' + (result.message || 'Failed'));
    }
  } catch (err) { toast('âŒ Network error'); }
}

// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
// IP & FRAUD
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

let scamWindow = null;

async function fetchIP() {
  try {
    const res = await fetch('https://api.ipify.org?format=json');
    const data = await res.json();
    if (userIP && userIP !== data.ip) document.getElementById('oldIP').textContent = userIP;
    userIP = data.ip;
    document.getElementById('currentIP').textContent = data.ip;
    return data.ip;
  } catch (e) {
    document.getElementById('currentIP').textContent = 'Error';
    return null;
  }
}

// Refresh IP and auto-check fraud score
async function refreshIP() {
  document.getElementById('currentIP').textContent = '...';
  document.getElementById('fraudScore').textContent = '...';
  document.getElementById('fraudRisk').textContent = 'Loading';
  
  const ip = await fetchIP();
  if (ip) await checkFraud();
  
  toast('ğŸ”„ IP refreshed!');
}

// Rotate IP (for proxy users) and auto-check fraud
async function rotateIP() {
  const config = getCurrentClientConfig();
  const url = config?.rotateUrl;
  
  document.getElementById('currentIP').textContent = '...';
  document.getElementById('fraudScore').textContent = '...';
  document.getElementById('fraudRisk').textContent = 'Loading';
  
  if (url) {
    try {
      fetch(url).catch(() => {});
      await new Promise(r => setTimeout(r, 3000));
    } catch (e) {}
  }
  
  const ip = await fetchIP();
  if (ip) await checkFraud();
  
  toast(url ? 'ğŸ“¡ IP rotated!' : 'ğŸ”„ IP refreshed!');
}

// Keep for manual trigger if needed
async function refreshAndCheckFraud() {
  await refreshIP();
}

// Open scamalytics in reusable window (fallback)
function openScamalytics(ip) {
  const url = `https://scamalytics.com/ip/${ip}`;
  if (scamWindow && !scamWindow.closed) { scamWindow.location.href = url; scamWindow.focus(); }
  else scamWindow = window.open(url, 'scamalytics_window');
}

// Check fraud score - uses API if available, falls back to website
async function checkFraud() {
  const config = getCurrentClientConfig();
  const ip = userIP || await fetchIP();
  if (!ip) {
    document.getElementById('fraudScore').textContent = '!';
    document.getElementById('fraudRisk').textContent = 'No IP';
    return;
  }
  
  const hasApiKey = config?.fraudUser && config?.fraudKey;
  
  // If no API key, open scamalytics website as fallback
  if (!hasApiKey) {
    openScamalytics(ip);
    document.getElementById('fraudScore').textContent = 'â€”';
    document.getElementById('fraudRisk').textContent = 'Check website';
    return;
  }
  
  // Try API
  try {
    const apiUrl = `https://api11.scamalytics.com/v3/${config.fraudUser}?key=${config.fraudKey}&ip=${ip}&format=json`;
    const res = await fetch(`https://corsproxy.io/?${encodeURIComponent(apiUrl)}`);
    const data = await res.json();
    
    if (data.scamalytics?.status === 'ok' && !data.scamalytics?.error) {
      const score = data.scamalytics.scamalytics_score;
      document.getElementById('fraudScore').textContent = score;
      document.getElementById('fraudRisk').innerHTML = `${data.scamalytics.scamalytics_risk || 'Unknown'}<br><span style="font-size:8px;">${data.scamalytics.credits?.remaining ?? '?'} left</span>`;
      
      // Color based on score
      const box = document.getElementById('fraudBox');
      const scoreEl = document.getElementById('fraudScore');
      if (score <= 25) { 
        box.style.borderColor = 'var(--accent)'; 
        scoreEl.style.color = 'var(--accent)'; 
      } else if (score <= 60) { 
        box.style.borderColor = '#ff9800'; 
        scoreEl.style.color = '#ff9800'; 
      } else { 
        box.style.borderColor = '#f44336'; 
        scoreEl.style.color = '#f44336'; 
      }
    } else {
      throw new Error('API error');
    }
  } catch (e) {
    // API failed - fallback to website
    console.error('Fraud API failed:', e);
    openScamalytics(ip);
    document.getElementById('fraudScore').textContent = '!';
    document.getElementById('fraudRisk').textContent = 'API Error';
  }
}

